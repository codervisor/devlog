{
    "servers": {
        // "github": {
        //     "url": "https://api.githubcopilot.com/mcp/"
        // },
        "devlog": {
            "type": "stdio",
            "command": "pnpm",
            "args": [
                "--filter",
                "@devlog/mcp",
                "start"
            ]
        },
        "playwright": {
            "command": "npx",
            "args": [
                "@playwright/mcp@latest",
                "--viewport-size", "1920,1080"
            ]
        },
        "git": {
            "command": "npx",
            "args": [
                "@cyanheads/git-mcp-server"
            ],
            "env": {
                "MCP_LOG_LEVEL": "debug",
                "GIT_SIGN_COMMITS": "false"
            }
        }
    }
}