{
	"servers": {
		"playwright": {
			"command": "npx",
			"args": [
				"@playwright/mcp@latest"
			],
			"type": "stdio"
		},
		"devlog": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"@codervisor/devlog-mcp@dev"
			]
		},
		"github": {
			"type": "http",
			"url": "https://api.githubcopilot.com/mcp/",
      "headers": {
        "Authorization": "Bearer ${env:GITHUB_TOKEN}"
      }
		}
	},
	"inputs": []
}