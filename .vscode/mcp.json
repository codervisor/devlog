{
	"servers": {
		"devlog": {
			"type": "stdio",
			"command": "pnpm",
			"args": [
				"--filter",
				"@codervisor/devlog-mcp",
				"dev:nodemon"
			],
			"cwd": ".",
			"env": {
				"NODE_ENV": "development",
				"MCP_MODE": "direct"
			}
		},
		"playwright": {
			"command": "npx",
			"args": [
				"@playwright/mcp@latest"
			],
			"type": "stdio"
		},
		// "jetbrains": {
		// 	"command": "npx",
		// 	"args": [
		// 		"-y",
		// 		"@jetbrains/mcp-proxy"
		// 	],
		// 	"type": "stdio"
		// }
	},
	"inputs": []
}