{
  "name": "@codervisor/test-utils",
  "version": "0.0.1",
  "description": "Shared test utilities, mocks, and factories for the devlog monorepo",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "keywords": [
    "testing",
    "test-utils",
    "mocks",
    "factories",
    "devlog"
  ],
  "author": {
    "name": "Marvin Zhang",
    "email": "tikazyq@163.com"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@codervisor/devlog-shared": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.1.9"
  },
  "peerDependencies": {
    "vitest": "^2.0.0"
  }
}
