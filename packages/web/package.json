{
  "name": "@codervisor/devlog-web",
  "version": "1.0.0",
  "description": "Web interface for devlog management",
  "scripts": {
    "dev": "next dev --port 3000",
    "dev:full": "concurrently --names \"CORE,WEB\" --prefix-colors \"green,blue\" \"pnpm --filter @codervisor/devlog-core dev\" \"pnpm dev\"",
    "build": "NEXT_BUILD_MODE=standalone next build",
    "build:test": "NEXT_BUILD_MODE=standalone next build --no-lint",
    "start": "pnpm run build && next start --port 3010",
    "preview": "next start --port 3010",
    "clean": "rimraf .next .next-build out *.tsbuildinfo",
    "clean:dev": "rimraf .next",
    "clean:build": "rimraf .next-build"
  },
  "dependencies": {
    "@codervisor/devlog-ai": "workspace:*",
    "@codervisor/devlog-core": "workspace:*",
    "@uiw/react-textarea-code-editor": "3.1.1",
    "classnames": "2.5.1",
    "highlight.js": "11.11.1",
    "next": "^14.0.4",
    "pg": "^8.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "10.1.0",
    "rehype-highlight": "7.0.2",
    "rehype-sanitize": "6.0.0",
    "remark-gfm": "4.0.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.2.6",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.11.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/ws": "^8.5.10",
    "antd": "^5.12.8",
    "autoprefixer": "^10.4.17",
    "concurrently": "9.2.0",
    "date-fns": "^3.2.0",
    "lucide-react": "^0.323.0",
    "postcss": "^8.4.33",
    "recharts": "^2.10.3",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "devlog",
    "development",
    "tracking",
    "web",
    "dashboard"
  ],
  "author": {
    "name": "Marvin Zhang",
    "email": "tikazyq@163.com"
  },
  "license": "Apache-2.0"
}
