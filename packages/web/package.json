{
  "name": "@codervisor/devlog-web",
  "version": "1.0.0",
  "description": "Web interface for devlog management",
  "scripts": {
    "dev": "next dev --port 3000",
    "dev:full": "concurrently --names \"CORE,WEB\" --prefix-colors \"green,blue\" \"pnpm --filter @codervisor/devlog-core dev\" \"pnpm dev\"",
    "build": "NEXT_BUILD_MODE=standalone next build",
    "build:test": "NEXT_BUILD_MODE=standalone next build --no-lint",
    "start": "pnpm run build && next start --port 3010",
    "preview": "next start --port 3010",
    "clean": "rimraf .next .next-build out *.tsbuildinfo",
    "clean:dev": "rimraf .next",
    "clean:build": "rimraf .next-build"
  },
  "dependencies": {
    "@codervisor/devlog-ai": "workspace:*",
    "@codervisor/devlog-core": "workspace:*",
    "@hookform/resolvers": "5.2.0",
    "@radix-ui/react-accordion": "1.2.11",
    "@radix-ui/react-alert-dialog": "1.1.14",
    "@radix-ui/react-checkbox": "1.3.2",
    "@radix-ui/react-dialog": "1.1.14",
    "@radix-ui/react-dropdown-menu": "2.1.15",
    "@radix-ui/react-label": "2.1.7",
    "@radix-ui/react-navigation-menu": "1.2.13",
    "@radix-ui/react-popover": "1.1.14",
    "@radix-ui/react-progress": "1.1.7",
    "@radix-ui/react-select": "2.2.5",
    "@radix-ui/react-separator": "1.1.7",
    "@radix-ui/react-slot": "1.2.3",
    "@radix-ui/react-switch": "1.2.5",
    "@radix-ui/react-tabs": "1.1.12",
    "@radix-ui/react-tooltip": "1.2.7",
    "@tailwindcss/typography": "0.5.16",
    "@uiw/react-textarea-code-editor": "3.1.1",
    "class-variance-authority": "0.7.1",
    "classnames": "2.5.1",
    "clsx": "2.1.1",
    "highlight.js": "11.11.1",
    "next": "^14.0.4",
    "pg": "^8.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.61.1",
    "react-markdown": "10.1.0",
    "rehype-highlight": "7.0.2",
    "rehype-sanitize": "6.0.0",
    "remark-gfm": "4.0.1",
    "sonner": "2.0.6",
    "tailwind-merge": "3.3.1",
    "tailwindcss-animate": "1.0.7",
    "typeorm": "0.3.25",
    "ws": "^8.14.2",
    "zod": "^3.25.67",
    "@tailwindcss/postcss": "4.1.11"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/pg": "^8.11.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/ws": "^8.5.10",
    "autoprefixer": "^10.4.21",
    "concurrently": "9.2.0",
    "date-fns": "^3.2.0",
    "lucide-react": "^0.323.0",
    "postcss": "^8.5.6",
    "recharts": "^2.10.3",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "devlog",
    "development",
    "tracking",
    "web",
    "dashboard"
